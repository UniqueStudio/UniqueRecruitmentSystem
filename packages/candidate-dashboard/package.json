    "@lingui/detect-locale": "^3.9.0",
    "@lingui/react": "^3.9.0",
    "@material-ui/core": "^5.0.0-alpha.36",
    "@material-ui/icons": "^5.0.0-alpha.36",
    "@material-ui/lab": "^5.0.0-alpha.36",
    "@material-ui/styles": "^5.0.0-alpha.35",
    "@reduxjs/toolkit": "^1.6.0",
    "@uniqs/apis": "workspace:*",
    "@uniqs/config": "workspace:*",
    "@uniqs/ui": "workspace:*",
    "@uniqs/utils": "workspace:*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
