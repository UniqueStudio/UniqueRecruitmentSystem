{
  "name": "@uniqs/candidate-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "extract": "NODE_ENV=development lingui extract --overwrite",
    "compile": "lingui compile",
    "start:dev": "node scripts/devServer",
    "start:prod": "node scripts/prodServer",
